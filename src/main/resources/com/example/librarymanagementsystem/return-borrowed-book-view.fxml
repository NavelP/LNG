<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="746.0" prefWidth="747.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.librarymanagementsystem.ReturnBorrowedBook">

    <children>
        <ToolBar fx:id="borrow_view_books" layoutX="14.0" layoutY="14.0" prefHeight="40.0" prefWidth="473.0">
            <items>
                <Button fx:id="return_view" mnemonicParsing="false" text="View Books" />
                <Button fx:id="return_read_books" layoutX="10.0" layoutY="13.0" mnemonicParsing="false" text="Read" />
                <Button fx:id="return_boroow_books" layoutX="76.0" layoutY="13.0" mnemonicParsing="false" text="Borrow" />
                <Button fx:id="return_return_books" layoutX="142.0" layoutY="13.0" mnemonicParsing="false" text="Return" />
                <MenuButton mnemonicParsing="false" text="Account Settings">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Action 1" />
                        <MenuItem mnemonicParsing="false" text="Action 2" />
                    </items>
                </MenuButton>
            </items>
        </ToolBar>
      <Label layoutX="240.0" layoutY="75.0" text="Return Book Form" />
      <TextField fx:id="return_national_id" layoutX="203.0" layoutY="113.0" promptText="Enter national id ..." />
      <TextField fx:id="return_book_name" layoutX="203.0" layoutY="185.0" promptText="Enter book name..." />
      <Button fx:id="return_button" layoutX="258.0" layoutY="257.0" mnemonicParsing="false" text="Return" />
    </children>

</AnchorPane>
